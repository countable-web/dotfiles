
alias perm="sudo chmod -R g+rw . && sudo chgrp -R dev . && find . -type d | sudo xargs chmod g+s"
# Strip whitespace
alias wstrip="sed 's/[\n ]//g'"
alias fiddle="sudo tcpdump -s 1024 -l -A dst"
alias dock-node="docker run -p 3000:3000 -it --rm -v $(pwd):/usr/src/app countable/node-dev bash"
alias attach-monitor="xrandr --output DP1 --left-of eDP1"

